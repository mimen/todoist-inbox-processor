{
  "name": "todoist-inbox-processor",
  "version": "1.0.0",
  "description": "A sleek Next.js interface for processing Todoist inbox tasks",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run redis\" \"next dev\" --names \"redis,next\" --prefix-colors \"red,blue\"",
    "dev:turbo": "concurrently \"npm run redis\" \"next dev --turbo\" --names \"redis,next\" --prefix-colors \"red,blue\"",
    "dev:auto": "concurrently \"npm run redis\" \"node scripts/dev-with-auto-clean.js\" --names \"redis,next\" --prefix-colors \"red,blue\"",
    "dev:clean": "npm run clean && npm run dev",
    "dev:next-only": "next dev",
    "redis": "redis-server --port 6379 --loglevel notice",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf .next",
    "restart": "pkill -f 'redis-server' || true && pkill -f 'next dev' || true && npm run dev:clean"
  },
  "keywords": [
    "todoist",
    "productivity",
    "nextjs",
    "task-management"
  ],
  "author": "David Miotti",
  "license": "MIT",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@doist/todoist-api-typescript": "^5.0.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.3.5",
    "google-auth-library": "^10.2.0",
    "googleapis": "^154.1.0",
    "lucide-react": "^0.525.0",
    "next": "^15.3.5",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redis": "^5.7.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
